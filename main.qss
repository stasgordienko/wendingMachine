QRadioButton::indicator::unchecked {
    width: 50px;
    height: 50px;
}

QRadioButton::indicator::checked {
    width: 50px;
    height: 50px;
}

QRadioButton::indicator::unchecked:pressed {
    width: 50px;
    height: 60px;
}


#oneside::indicator::unchecked, #doubleside::indicator::unchecked, #flash_choose::indicator::unchecked, #email_choose::indicator::unchecked {
	background-image: url("img/radio.png");
	background-color: transparent;
	border-width: 0px;
	border: none;
        color: white;
        width: 90px;
        height: 90px;
}

