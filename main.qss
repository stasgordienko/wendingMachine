#b_en,#b_ua,#b_ru {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 white, stop:0.5 rgba(113,170,222,200), stop:1 rgba(113,170,222,250)); 
border: 3px solid rgba(113,170,222,200); 
border-radius: 10px; color: white;
}

#b_en:checked,#b_ua:checked,#b_ru:checked,#b_en:pressed,#b_ua:pressed,#b_ru:pressed {
background: qlineargradient(x1:1, y1:1, x2:1, y2:0, stop:0 white, stop:0.5 rgba(113,170,222,200), stop:1 rgba(113,170,222,250)); 
border: 3px solid rgba(113,170,222,200); 
border-radius: 10px; color: white;
}

#b_email,#b_help_main {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 white, stop:0.5 rgba(255,170,60,200), stop:1 rgba(255,170,60,250)); 
border: 3px solid rgba(255,170,60,200); 
border-radius: 10px; color: white;
}

#b_email:pressed, #b_help_main:pressed {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(255,170,60,250), stop:0.5 rgba(255,170,60,200), stop:1 white); 
border: 3px solid rgba(255,170,60,200); 
border-radius: 10px; color: white;
}


#b_price:pressed, #b_bal:pressed {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(33,152,17,250), stop:0.5 rgba(33,152,17,200), stop:1 white); 
border: 3px solid rgba(33,152,17,200); 
border-radius: 10px; color: white;
}

#b_price, #b_bal {
background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 white, stop:0.5 rgba(33,152,17,200), stop:1 rgba(33,152,17,250)); 
border: 3px solid rgba(33,152,17,200); 
border-radius: 10px; color: white;
}

